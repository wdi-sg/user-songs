<p>Create New Song</p>

<%= form_with scope: :song, url: songs_path, local: true do |form| %>
	<p>
		<%= form.label :title %><br>
		<%= form.text_field :title %>
	</p>

	<p>
		<%= form.submit %>
	</p>
<% end %>

<%= link_to 'Back', songs_path %>
<%= link_to 'log out', destroy_user_session_url, method: :delete %>
